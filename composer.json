{
	"name": "rtconner/laravel-kint",
	"description": "Seamless integration between Laravel and Kint.",
	"license": "MIT",
	"homepage": "https://smartersoftware.net/packages/laravel-kint",
	"keywords": ["laravel", "kint", "laravel5"],
	"authors": [
		{
			"name": "Robert Conner",
			"email": "rtconner@smarter.bz"
		}
	],
	"require": {
		"php": ">=5.3",
		"kint-php/kint": "~2.0"
	},
	"autoload": {
		"psr-4": {
			"Conner\\Kint\\": "src/"
		}
	},
	"scripts": {
        "post-install-cmd": [
            "php -f ./defineKintDir.php"
        ],
        "pre-update-cmd": [
            "php -f ./defineKintDir.php"
        ]
    },
	"minimum-stability": "dev"
}
